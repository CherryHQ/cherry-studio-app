{
  "cli": {
    "version": ">= 16.3.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "env": {
          "BUNDLE_ID": "com.cherry-studio.app"
        }
      },
      "android": {
        "env": {
          "BUNDLE_ID": "com.cherry_studio.app"
        }
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}